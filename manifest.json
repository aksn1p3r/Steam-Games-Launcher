{
"manifest_version": 2,
"name": "Valve Steam Games Launcher",
"description": "Launch Free to Play Valve Steam Games",
"version": "1.3",
"icons": {	"16": "img/lambda_logo_16.png",
		"48": "img/lambda_logo_48.png",
		"128": "img/lambda_logo_128.png" },
"permissions": ["contextMenus", "storage"],
"omnibox": { 	"keyword" : "i" },

"background": {	"scripts": ["js/background.js"]
	},
"content_security_policy": "default-src 'self'",
"incognito": "split",
"options_page": "popup.html",
"browser_action": { "38": "img/lambda_logo_38.png", 
          "default_title": "Valve Steam Games Launcher", 
          "default_popup": "popup.html" 
        }
}